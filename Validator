<html>
<head>
<script>
var c=0;
 function validate()
            {
                var a=parseInt(document.getElementById('input1').value);
                var b=parseInt(document.getElementById('input2').value);
                if(a>=18 && b>=21)
                {
                    document.getElementById('result').innerHTML="Valid Marriage";
                }
                else
                {
                    document.getElementById('result').innerHTML="Invalid Marriage";
                }
            }
</script>
</head>
<body>
        <div align="center">
            <h1>Marriage Validator</h1>
            <form>
                Bride's Age: <input type='number' id='input1'><br><br>
                Groom's Age: <input type='number' id='input2'><br><br>
                <input type="button" value="Validate" onclick="validate()">
                
            </form>
            <h2 id='result'></h2>
        </div>
        
    </body>
</html><html><head><META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=utf-8"><meta name="Robots" content="NOINDEX " /></head><body></body>
